    --</span><span class="hl-reserved">border-styling:</span><span class="hl-code"> </span><span class="hl-string">solid</span><span class="hl-code"> </span><span class="hl-number">.0625</span><span class="hl-code">rem rgb(var(--swatch-border-color, var(--medium-accent, </span><span class="hl-number">228</span><span class="hl-code">, </span><span class="hl-number">180</span><span class="hl-code">, </span><span class="hl-number">28</span><span class="hl-code">)))</span><span class="hl-code">;
    --</span><span class="hl-reserved">sidebar-internal-border-thickness:</span><span class="hl-code"> </span><span class="hl-number">.0625</span><span class="hl-code">rem</span><span class="hl-code">;
